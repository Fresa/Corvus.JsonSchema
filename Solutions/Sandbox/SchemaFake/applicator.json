{
  "$schema": "https://json-schema.org/draft/2020-12/schema",

  "title": "Schema vocabulary meta-schema",
  "type": [ "object", "boolean" ],
  "properties": {
    "additionalItems": { "$ref": "#" },
    "unevaluatedItems": { "$ref": "#" },
    "items": {
      "anyOf": [
        { "$ref": "#" },
        { "$ref": "#/$defs/schemaArray" }
      ]
    }
  },
  "$defs": {
    "schemaArray": {
      "type": "array",
      "minItems": 1,
      "items": { "$ref": "#" }
    }
  }
}
